@model IList<Domain.Models.Problem>

<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Problem Name</th>
            <th>Difficulty level</th>    
        </tr>
    </thead>
    <tbody>
        @foreach (var problem in Model)
        {
            <tr>
                <td>@problem.Id</td>
                <td>@problem.Name</td>
                <td>@problem.Difficulty</td>
            </tr>
        }
    </tbody>
</table>